{
	"extends": "@vue/tsconfig/tsconfig.json",
	"compilerOptions": {
		"outDir": "dist",
		"target": "esnext",
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUncheckedIndexedAccess": false,
		"exactOptionalPropertyTypes": false,
		"skipLibCheck": true
	}
}
